<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import temporizador.Temporizador?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appusotemporizador.TemporizadorController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <Label text="Temporizador:" textFill="#790000">
               <font>
                  <Font name="Georgia Bold" size="33.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></Label>
            <Temporizador fx:id="temporizador" onAction="#finalizaTemporizador">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Temporizador>
         </children>
      </VBox>
   </children>
</AnchorPane>
